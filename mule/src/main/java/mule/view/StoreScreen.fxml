            </Label>
            <Separator maxWidth="75.0" prefWidth="75.0" />
            <Label alignment="TOP_LEFT" prefHeight="75.0" prefWidth="75.0" text="Label">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <Separator maxWidth="75.0" prefWidth="75.0" />
            <Label alignment="TOP_LEFT" prefHeight="75.0" prefWidth="75.0" text="Label">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <Separator maxWidth="75.0" prefWidth="75.0" />
            <Label fx:id="timerLabel" alignment="CENTER" prefHeight="75.0" prefWidth="75.0" text="60">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </items>
      </ToolBar>
      <ChoiceBox prefHeight="15.0" prefWidth="75.0" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets bottom="215.0" right="610.0" />
         </StackPane.margin>
      </ChoiceBox>
      <ChoiceBox fx:id="oreQuantity" prefHeight="15.0" prefWidth="75.0">
         <StackPane.margin>
            <Insets bottom="215.0" right="385.0" />
