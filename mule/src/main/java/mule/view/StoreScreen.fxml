      </ChoiceBox>
      <ChoiceBox fx:id="foodQuantity" prefHeight="15.0" prefWidth="75.0">
         <StackPane.margin>
            <Insets bottom="215.0" right="160.0" />
         </StackPane.margin>
      </ChoiceBox>
      <ChoiceBox fx:id="energyQuantity" prefHeight="15.0" prefWidth="75.0">
         <StackPane.margin>
            <Insets bottom="215.0" left="65.0" />
         </StackPane.margin>
      </ChoiceBox>
   </children>
</StackPane>
