            <Insets bottom="140.0" right="50.0" />
         </StackPane.margin></Button>
      <Button fx:id="foodMule" mnemonicParsing="false" onAction="#buyFoodMule" prefWidth="138.0" text="Food M.U.L.E." StackPane.alignment="CENTER_RIGHT">
