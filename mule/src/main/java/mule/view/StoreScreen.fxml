            <Insets bottom="415.0" right="125.0" />
         </StackPane.margin></Button>
      <Button fx:id="energyMule" mnemonicParsing="false" onAction="#buyEnergyMule" text="Energy M.U.L.E" StackPane.alignment="CENTER_RIGHT">
