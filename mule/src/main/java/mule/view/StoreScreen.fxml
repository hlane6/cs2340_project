<StackPane id="store-pane" prefHeight="475.0" prefWidth="760.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mule.StoreController">
   <children>
      <Button fx:id="buyButton" mnemonicParsing="false" onAction="#buyResource" text="Buy" StackPane.alignment="BOTTOM_CENTER">
         <StackPane.margin>
            <Insets bottom="140.0" right="200.0" />
         </StackPane.margin></Button>
      <Button fx:id="sellButton" mnemonicParsing="false" onAction="#sellResource" text="Sell" StackPane.alignment="BOTTOM_CENTER">
