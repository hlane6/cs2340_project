<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="375.0" prefWidth="675.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mule.StoreController">
   <children>
      <ToolBar fx:id="infoBar" prefHeight="40.0" prefWidth="675.0" StackPane.alignment="BOTTOM_CENTER">
         <items>
            <Label prefWidth="75.0" text="Player 1:" />
            <Label text="Player 2:" />
            <Label prefWidth="75.0" text="Player 3:" />
            <Label prefWidth="75.0" text="Player 4:" />
            <Separator orientation="VERTICAL" prefHeight="25.0" />
            <Label fx:id="mapText" prefHeight="15.0" prefWidth="175.0" text="New Turn" />
            <Separator orientation="VERTICAL" prefHeight="25.0" />
            <Label fx:id="timerLabel" prefWidth="75.0" text="Time Left: " />
         </items>
      </ToolBar>
      <ChoiceBox fx:id="resourceChoiceBox" prefHeight="15.0" prefWidth="109.0" StackPane.alignment="CENTER_LEFT">
         <StackPane.margin>
            <Insets left="50.0" />
         </StackPane.margin></ChoiceBox>
      <ChoiceBox fx:id="quantityChoiceBox" prefHeight="15.0" prefWidth="109.0" StackPane.alignment="CENTER" />
      <Label prefHeight="19.0" prefWidth="60.0" text="Resource" StackPane.alignment="CENTER_LEFT">
         <StackPane.margin>
            <Insets bottom="50.0" left="50.0" />
         </StackPane.margin></Label>
      <Label text="Quantity" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets bottom="50.0" />
         </StackPane.margin></Label>
      <Button fx:id="buyButton" mnemonicParsing="false" text="Buy" StackPane.alignment="CENTER_RIGHT">
         <StackPane.margin>
            <Insets bottom="50.0" right="100.0" />
         </StackPane.margin></Button>
      <Button fx:id="sellButton" mnemonicParsing="false" text="Sell" StackPane.alignment="CENTER_RIGHT">
         <StackPane.margin>
            <Insets right="100.0" top="50.0" />
         </StackPane.margin></Button>
      <Label text="M.U.L.E">
         <StackPane.margin>
            <Insets top="100.0" />
         </StackPane.margin></Label>
      <Button fx:id="foodMule" mnemonicParsing="false" text="Food M.U.L.E.">
         <StackPane.margin>
            <Insets right="300.0" top="200.0" />
         </StackPane.margin></Button>
      <Button fx:id="energyMule" mnemonicParsing="false" text="Energy M.U.L.E">
         <StackPane.margin>
            <Insets top="200.0" />
         </StackPane.margin></Button>
      <Button fx:id="oreMule" mnemonicParsing="false" text="Ore M.U.L.E">
         <StackPane.margin>
            <Insets left="300.0" top="200.0" />
         </StackPane.margin></Button>
      <Label prefHeight="34.0" text="Store" StackPane.alignment="TOP_CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <StackPane.margin>
            <Insets top="50.0" />
         </StackPane.margin>
      </Label>
   </children>
</StackPane>
